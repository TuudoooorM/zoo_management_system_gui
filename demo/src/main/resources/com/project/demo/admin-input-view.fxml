                  <children>
                     <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                        <children>
                           <Label minWidth="150.0" text="First name" textFill="#8ade7c">
                              <font>
                                 <Font name="Montserrat Black" size="16.0" />
                              </font>
                           </Label>
                           <TextField fx:id="firstNameInput" maxWidth="1.7976931348623157E308" promptText="Enter admin's first name" style="-fx-border-radius: 30; -fx-background-color: transparent; -fx-border-color: #58FF4E; -fx-border-width: 2px; -fx-padding: 10px;" HBox.hgrow="ALWAYS">
                              <font>
                                 <Font name="Montserrat Regular" size="12.0" />
                              </font>
                           </TextField>
                        </children>
                     </HBox>
                     <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="89.0" prefHeight="99.0" prefWidth="317.0" spacing="50.0">
                        <children>
                           <Label minWidth="150.0" text="Last name" textFill="#8ade7c">
                              <font>
                                 <Font name="Montserrat Black" size="16.0" />
                              </font>
                           </Label>
                           <TextField fx:id="lastNameInput"  maxWidth="1.7976931348623157E308" promptText="Enter admin's last name" style="-fx-border-radius: 30; -fx-background-color: transparent; -fx-border-color: #58FF4E; -fx-border-width: 2px; -fx-padding: 10px;" HBox.hgrow="ALWAYS">
                              <font>
                                 <Font name="Montserrat Regular" size="12.0" />
                              </font>
                           </TextField>
                        </children>
                     </HBox>
                     <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="24.0" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                        <children>
                           <Label minWidth="150.0" text="Sex" textFill="#8ade7c">
                              <font>
                                 <Font name="Montserrat Black" size="16.0" />
                              </font>
                           </Label>
                            <ChoiceBox fx:id="sexChoiceInput" maxWidth="1.7976931348623157E308" style="-fx-border-radius: 30; -fx-background-color: transparent; -fx-border-color: #58FF4E; -fx-border-width: 2px; -fx-padding: 4px;" value="Male" HBox.hgrow="ALWAYS">
                                <items>
                                    <FXCollections fx:factory="observableArrayList">
                                        <String fx:value="Male" />
                                        <String fx:value="Female" />
                                    </FXCollections>
                                </items>
                            </ChoiceBox>
                        </children>
                     </HBox>
                     <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="154.0" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                        <children>
                           <Label minWidth="150.0" text="Salary" textFill="#8ade7c">
                              <font>
                                 <Font name="Montserrat Black" size="16.0" />
                              </font>
                           </Label>
                           <TextField fx:id="yearlySalaryInput"  maxWidth="1.7976931348623157E308" promptText="Enter admin's yearly salary here" style="-fx-border-radius: 30; -fx-background-color: transparent; -fx-border-color: #58FF4E; -fx-border-width: 2px; -fx-padding: 10px;" HBox.hgrow="ALWAYS">
                              <font>
                                 <Font name="Montserrat Regular" size="12.0" />
                              </font>
                           </TextField>
                        </children>
                     </HBox>
                     <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                        <children>
                           <Label minWidth="150.0" text="Worked months" textFill="#8ade7c">
                              <font>
                                 <Font name="Montserrat Black" size="16.0" />
                              </font>
                           </Label>
                           <TextField fx:id="workedMonthsInput"  maxWidth="1.7976931348623157E308" promptText="Enter admin's number of worked months" style="-fx-border-radius: 30; -fx-background-color: transparent; -fx-border-color: #58FF4E; -fx-border-width: 2px; -fx-padding: 10px;" HBox.hgrow="ALWAYS">
                              <font>
                                 <Font name="Montserrat Regular" size="12.0" />
                              </font>
                           </TextField>
                        </children>
                     </HBox>
                     <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="94.0" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                        <children>
                           <Label minWidth="150.0" text="Password" textFill="#8ade7c">
                              <font>
                                 <Font name="Montserrat Black" size="16.0" />
                              </font>
                           </Label>
                           <PasswordField fx:id="passwordInput"  maxWidth="1.7976931348623157E308" promptText="Enter the admin password" style="-fx-border-radius: 30; -fx-background-color: transparent; -fx-border-color: #58FF4E; -fx-border-width: 2px; -fx-padding: 10px;" HBox.hgrow="ALWAYS">
                              <font>
                                 <Font name="Montserrat Regular" size="12.0" />
                              </font>
